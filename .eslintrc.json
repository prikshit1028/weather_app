{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base",
    "prettier"
],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-inner-declarations": "off",
        "no-console": "off",
        "no-use-before-define": "warn"
    }
}
